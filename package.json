{
  "name": "@tatoonz/domql",
  "version": "1.0.0-alpha.0",
  "devDependencies": {
    "@types/assert": "^0.0.31",
    "@types/body-parser": "^0.0.33",
    "@types/cheerio": "^0.17.31",
    "@types/express": "^4.0.35",
    "@types/lodash": "^4.14.52",
    "@types/mocha": "^2.2.38",
    "@types/node": "^7.0.4",
    "@types/node-fetch": "^1.6.7",
    "concurrently": "^3.1.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "ts-node": "^2.0.0",
    "tslint": "^4.4.2",
    "typescript": "^2.1.5"
  },
  "dependencies": {
    "body-parser": "^1.16.0",
    "cheerio": "^0.22.0",
    "express": "^4.14.1",
    "graphql": "^0.9.1",
    "graphql-server-express": "^0.6.0",
    "graphql-subscriptions": "^0.2.3",
    "graphql-tools": "^0.9.2",
    "lodash": "^4.17.4",
    "node-fetch": "^1.6.3"
  },
  "license": "MIT",
  "scripts": {
    "cleanup": "rm -rf ./lib",
    "compile": "tsc",
    "compile:watch": "tsc -w",
    "start": "node .lib/index.js",
    "start:watch": "nodemon --watch ./lib --ignore './lib/**/*.spec.js' -e js ./lib/index.js",
    "dev": "concurrently --kill-others \"npm run compile:watch\" \"npm run start:watch\"",
    "test": "mocha ./lib/**/*.spec.js",
    "test:watch": "nodemon --watch ./lib -e spec.js --exec npm run test",
    "postinstall": "npm run cleanup && npm run compile",
    "lint": "tslint './src/**/*.ts",
    "lint:watch": "nodemon --watch ./src -e ts --exec npm run lint"
  }
}
